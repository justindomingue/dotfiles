[user]
	name = justindomingue
	email = justin.domingue@hotmail.com
[push]
	default = current
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[credential]
	helper = osxkeychain

[alias]
  aa = add -A
  cm = commit -m
  cam = !git add -A && git commit -m
  s = status -s
  ss = status
  d = !"git diff --minimal --patch-with-stat"
  c = clone
  p = push
  l = !"git --no-pager log --pretty=oneline -n 20 --graph"
  ctags = "!.git/hooks/ctags"
[init]
	templatedir = /Users/justindomingue/.git_template
